{
  "name": "hmiui",
  "version": "0.0.0",
  "scripts": {
    "reset": "rimraf ./node_modules",
    "ng": "ng",
    "start": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check --poll 1000",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:api": "rimraf src/app/core/api/generated && openapi-generator generate -i src/app/core/api/api.json -g typescript-angular -o src/app/core/api/generated --additional-properties useSingleRequestParameter=true",
    "generate:i18n": "ngx-translate-extract --input src --output src/assets/i18n/en.json src/assets/i18n/es.json --clean --sort --null-as-default-value --format namespaced-json",
    "build:prod": "ng build --configuration production --output-path=./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.1",
    "@angular/cdk": "^13.2.1",
    "@angular/common": "~13.2.1",
    "@angular/compiler": "~13.2.1",
    "@angular/core": "~13.2.1",
    "@angular/forms": "~13.2.1",
    "@angular/material": "^13.2.1",
    "@angular/platform-browser": "~13.2.1",
    "@angular/platform-browser-dynamic": "~13.2.1",
    "@angular/router": "~13.2.1",
    "@google/model-viewer": "^1.2.1",
    "@microsoft/signalr": "^3.1.9",
    "@ng-select/ng-select": "^5.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-gridster2": "^10.1.5",
    "angular-resize-event": "^2.0.0",
    "d3": "^6.1.1",
    "ng-block-ui": "^3.0.2",
    "ng5-slider": "^1.2.4",
    "ngx-popper": "^7.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5",
    "rimraf": "^3.0.2"
  }
}