{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/HMI.API",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:51988",
      "sslPort": 44334
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "LOG_LEVEL": "1",
        "OPCUA_MODE": "0",
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "HMI.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "LOG_LEVEL": "1",
        "OPCUA_MODE": "0",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true,

      "environmentVariables": {
        "LOG_LEVEL": "1",
        "OPCUA_MODE": "0",
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_HTTPS_PORT": "44360"
      },
      "httpPort": 51803,
      "sslPort": 44360
    }
  }
}